{
  "manifest_version": 3,
  "name": "광고 제거",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["contentScript.js"],
    "run_at": "document_end"
  }],
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  }
}
